Tab4:AddLabel("Player")

Tab4:AddButton({
	Name = "Reset Button",
	Callback = function()
      		game.Players.LocalPlayer.Character.Humanoid.Health = 0
  	end    
})

Tab4:AddTextbox({
	Name = "Walk Speed",
	Default = "Enter Number",
	TextDisappear = true,
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
	end	  
})

Tab4:AddTextbox({
	Name = "Jump Power",
	Default = "Enter Number",
	TextDisappear = true,
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
	end	  
})

Tab4:AddTextbox({
	Name = "FOV",
	Default = "Enter Number",
	TextDisappear = true,
	Callback = function(txt)
		game.Workspace.CurrentCamera.FieldOfView = txt
	end	  
})

Tab4:AddTextbox({
	Name = "Gravity",
	Default = "Enter Number",
	TextDisappear = true,
	Callback = function(newGravity)
		game.Workspace.Gravity = newGravity
	end	  
})

Tab4:AddLabel("Server")

Tab4:AddButton({
	Name = "Rejoin",
	Callback = function()
      		local serverId = game.JobId
    game:GetService("TeleportService"):TeleportToPlaceInstance(game.PlaceId, serverId, player)
  	end    
})

Tab4:AddButton({
	Name = "Players In Server",
	Callback = function()
      		-- Create a ScreenGui to hold the GUI elements
local gui = Instance.new("ScreenGui")
gui.Parent = game.Players.LocalPlayer.PlayerGui

-- Create a Frame inside the ScreenGui
local frame = Instance.new("Frame")
frame.Size = UDim2.new(1, 0, 0.1, 0)
frame.Position = UDim2.new(0, 0, 0.9, 0)
frame.BackgroundTransparency = 0.5
frame.BackgroundColor3 = Color3.new(0, 0, 1)
frame.Parent = gui

-- Create a Label inside the Frame
local label = Instance.new("TextLabel")
label.Size = UDim2.new(0.8, 0, 1, 0)
label.Position = UDim2.new(0.05, 0, 0, 0)
label.BackgroundTransparency = 1
label.FontSize = Enum.FontSize.Size18
label.TextColor3 = Color3.new(1, 1, 1)
label.Parent = frame

-- Create a Close button inside the Frame
local closeButton = Instance.new("TextButton")
closeButton.Size = UDim2.new(0.1, 0, 1, 0)
closeButton.Position = UDim2.new(0.85, 0, 0, 0)
closeButton.Text = "Close"
closeButton.FontSize = Enum.FontSize.Size18
closeButton.TextColor3 = Color3.new(1, 1, 1)
closeButton.BackgroundColor3 = Color3.new(0.5, 0.5, 0.5)
closeButton.Parent = frame

-- Function to update the label with player count
local function updatePlayerCount()
    local playerCount = #game.Players:GetPlayers()
    label.Text = "Players in server: " .. playerCount
end

-- Function to animate the frame fade-out
local function animateFadeOut()
    for i = 1, 10 do
        frame.BackgroundTransparency = i / 10
        wait(0.05)
    end
    gui:Destroy()
end

-- Connect the button to close the GUI and trigger animation
closeButton.MouseButton1Click:Connect(function()
    animateFadeOut()
end)

-- Initial update and update every second
updatePlayerCount()
while wait(1) do
    updatePlayerCount()
end

  	end    
})

Tab4:AddLabel("Items")

Tab4:AddButton({
	Name = "Equip Tool",
	Callback = function()
      		game.Players.LocalPlayer.Backpack:FindFirstChildOfClass("Tool").Parent = game.Players.LocalPlayer.Character
  	end    
})

Tab4:AddButton({
	Name = "Drop Tool",
	Callback = function()
      		game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool").Parent = game.Workspace
  	end    
})

